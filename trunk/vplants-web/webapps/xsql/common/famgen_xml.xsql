<?xml version="1.0"?>

<FAMGEN connection="{@dbconnection}" xmlns:xsql="urn:oracle-xsql">

<xsql:include-param name="fam"/>

<GENERA>
<xsql:query rowset-element="" row-element="">
SELECT DISTINCT
	CHK.GENUS
FROM
	SPECIMEN SPM,
	CHECKLIST GRP,
	CHECKLIST CHK
WHERE
	CHK.FAMILY_NAME = '{@fam}' AND
	CHK.GROUP_ID = GRP.GROUP_ID AND
	GRP.NAME_CD = SPM.NAME_CD
ORDER BY
        CHK.GENUS
</xsql:query>
</GENERA>

</FAMGEN>
